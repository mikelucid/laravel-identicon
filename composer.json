{
    "name": "mikeluicd/laravel-identicon",
    "description": "Laravel Identicon Package",
    "keywords": ["identicon", "avatar", "image", "laravel identicon", "laravel"],
    "homepage": "https://github.com/mikelucid/laravel-identicon/",
    "license": "MIT",
    "authors": [
        {
            "name": "Mike Lucid",
            "email": "mikelucid@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2 || ^8.1",
        "laravel/framework": "^8.0 || ^9.0",
        "yzalis/identicon": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Mikelucid\\LaravelIdenticon\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mikelucid\\LaravelIdenticon\\IdenticonServiceProvider"
            ],
            "aliases": {
                "Identicon": "Mikelucid\\LaravelIdenticon\\IdenticonFacade"
            }
        }
    }
}
